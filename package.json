    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-session": "^1.17.0",
    "fs-extra": "^9.0.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "mocha": "^7.1.2",
    "mongoose": "^6.2.7",
    "mongoose-seed": "^0.6.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
